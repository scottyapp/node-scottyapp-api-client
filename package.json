{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "scottyapp-api-client",
  "description": "Official node.js API client for scottyapp.com",
  "version": "0.0.1",
  "main" : "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/scottyapp/node-scottyapp-api-client.git"
  },
  "bin" : {
  },
  "directories" : {
    "lib" : "./lib",
    "test" : "./spec",
    "bin" : "./bin"
  },
  "keywords" : [
    "scottyapp","api","rest","restful","client"
  ],
  "scripts": {
    "test": "node_modules/.bin/vows --spec spec/*-spec.coffee",
    "watch" : "coffee -c -w  -o lib src"
  },
  "engines": {
    "node": ">=0.4.10"
  },
  "dependencies": {
    "underscore" : "1.2.x",
    "async" : "0.1.x",
    "colors" : "~ 0.5",
    "pkginfo" : "~ 0.2",
    "request": "2.1.x"
  },
  "devDependencies": {
    "should" : ">=0.2.1",    
    "coffee-script" : ">= 1.1.2",
    "vows" : ">= 0.5.9",
    "nock" : "*"
  },
  "bugs": { "url": "http://github.com/scottyapp/node-scottyapp-api-client/issues" },
  "licenses": [
    { "type": "MIT",
      "url": "http://github.com/scottyapp/node-scottyapp-api-client/raw/master/LICENSE"
    }
  ]
}
